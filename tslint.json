{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js"
    ]
  },
  "rules": {
    "max-line-length": {
      "options": [120]
    },
    "quotemark": {
      "options": ["single", "jsx-double"]
    },
    "ordered-imports": false,
    "no-consecutive-blank-lines": true
  }
}
